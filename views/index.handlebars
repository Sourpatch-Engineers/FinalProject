    <div class="div-view">
        <div>
            <link rel="stylesheet" href="/styling/index.css"/>
        </div>

        <div>
            <h1>Home Page</h1>

            <a href="/team_form"><button>Create Team</button></a>
        </div>

        <div id="teamsTable">
            <table>

                <thead>
                    <tr>
                        <th>Team Name</th>
                        <th>Team Leader</th>
                        <th>Team Members</th>
                    </tr>
                </thead>

                {{#each tableData}}
                    <tr>
                        <td>{{this.team_name}}</td>
                        <td>{{this.scrum_master}}</td>
                        <td>{{concat_members this.member_names}}</td>
                    </tr>
                {{/each}}

            </table>
        </div>
        
    </div>